        body[yahoo] td[class=3D"pattern"] .column:first-child {
            margin-right: 10px;
        }
        body[yahoo] td[class=3D"pattern"] .row:last-child .column:last-chil=
d {
            margin-bottom: 0;
            margin-right: 10px;
        }
        /* X-Site CSS */
        body[yahoo] =09td[class=3D"x-site"] {
            display: none;
        }
        /* STRIPE Switch */

        body[yahoo] .stripe-switch {
            width: 300px;
            height: 150px;
            background-image: url(http://g-ec2.images-amazon.com/images/G/0=
1/mobile/2013/300x150/300x150_v3.gif);
            display:block;
            background-repeat:no-repeat;
            background-color:#FFF;
            background-position:center;
        }
        body[yahoo] td[class=3D"stripe"] {
            text-align:center;
            display:block;
            background-color:#FFF;
            margin:none;
            width: 300px;
        }
        body[yahoo] td[class=3D"stripe"] img {
            display:none;
        }
        body[yahoo] td[class=3D"origin"]{
            max-height: 0px; font-size: 0; display: none;
        }

        body[yahoo] td[class=3D"origin"] img{
            max-height: 0px; font-size: 0; display: none;
        }
        /* Link Farm CSS */
        body[yahoo] table[class=3D"farm"] {
            display: none;
        }
        body[yahoo] td[class=3D"mobileShow"] img, body[yahoo] .mobileShow {
            display: block !important;
            width: 300px !important;
            height: auto !important;
            max-height: inherit !important;
            opacity: 1 !important;
        }

        body[yahoo] .textSegway .mobileShow {
            width: auto !important;
            font-family:arial,sans-serif;font-size:12px !important;color:#3=
36699;text-decoration:underline;font-weight:bold;
            margin-top: 3px;
        }

        body[yahoo] td[class=3D"textSegway"] {
            text-align: right !important;
        }

        }
